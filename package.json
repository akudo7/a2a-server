{
  "name": "a2a-server",
  "version": "1.0.0",
  "description": "Production-ready A2A Protocol Server with dual protocol support (HTTP REST + JSON-RPC 2.0), built on SceneGraphManager v2.0.0",
  "main": "dist/server.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/akudo7/a2a-server.git"
  },
  "bugs": {
    "url": "https://github.com/akudo7/a2a-server/issues"
  },
  "homepage": "https://github.com/akudo7/a2a-server#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx src/index.tsx",
    "build": "tsc",
    "start": "node dist/index.js",
    "server": "node dist/server.js",
    "server:dev": "tsx src/server.ts",
    "server:task": "node dist/server.js ./json/a2a/servers/task-creation.json",
    "server:research": "node dist/server.js ./json/a2a/servers/research-execution.json",
    "server:quality": "node dist/server.js ./json/a2a/servers/quality-evaluation.json",
    "server:main": "node dist/server.js ./json/a2a/main.json",
    "audit": "yarn audit",
    "audit:fix": "yarn audit --fix",
    "audit:report": "yarn audit --json > audit-report.json",
    "security:check": "yarn audit && yarn outdated",
    "security:full": "yarn audit --audit-level=moderate && yarn outdated --depth=0"
  },
  "keywords": [
    "a2a",
    "agent",
    "ai",
    "langchain",
    "langgraph",
    "workflow",
    "json",
    "multi-agent",
    "mcp",
    "anthropic",
    "openai"
  ],
  "license": "MIT",
  "author": {
    "name": "Akira Kudo",
    "email": "akudo7@gmail.com",
    "url": "https://www.linkedin.com/in/akira-kudo-4b04163/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@1.22.0",
  "dependencies": {
    "@a2a-js/sdk": "^0.3.7",
    "@kudos/scene-graph-manager": "file:/Users/akirakudo/Desktop/MyWork/TypeScript/SceneGraphManager/kudos-scene-graph-manager-2.0.0.tgz",
    "abort-controller": "^3.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.2.1",
    "formdata-node": "^6.0.3",
    "node-fetch": "^3.3.2",
    "web-streams-polyfill": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^25.0.3",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
